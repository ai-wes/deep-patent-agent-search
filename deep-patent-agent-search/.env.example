# ============================================
# AI Studio (Default) - Recommended for quick start
# ============================================
# Get your API key from https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=YOUR_AI_STUDIO_API_KEY_HERE

# ============================================
# Vertex AI (Alternative) - For Google Cloud users
# ============================================
# Uncomment to use Vertex AI instead of AI Studio.
# Project and location are auto-detected from your default credentials.
# Run: gcloud auth application-default login

# GOOGLE_GENAI_USE_VERTEXAI=TRUE

# ============================================
# Trace Persistence Configuration
# ============================================
# Configure how agent events and traces are persisted for traceability and reproducibility

# Storage type: "local" or "gcs"
TRACE_STORAGE_TYPE=local

# For local storage (default)
TRACE_LOCAL_PATH=agent_traces

# For GCS storage
# TRACE_GCS_BUCKET_NAME=your-bucket-name
# TRACE_GCS_PREFIX=traces

